i[class^="icon"]
  display inline-block
  vertical-align middle
  background-image url("../images/sprite.png")
  background-repeat no-repeat
  margin-right 5px
  retina()
.icon-map
  width 28px
  height 31px
  background-position 0 0
.icon-video
  width 28px
  height 18px
  background-position -50px 0